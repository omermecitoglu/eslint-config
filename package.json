{
  "name": "@omer-x/eslint-config",
  "version": "2.0.0",
  "description": "My favorite eslint rules",
  "keywords": [
    "eslint-config",
    "shareable-config"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omermecitoglu/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/omermecitoglu/eslint-config/issues"
  },
  "homepage": "https://github.com/omermecitoglu/eslint-config#readme",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Omer Mecitoglu",
    "email": "omer.mecitoglu@gmail.com",
    "url": "https://omermecitoglu.github.io"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./base": {
      "import": "./dist/base.js"
    },
    "./stylistic": {
      "import": "./dist/stylistic.js"
    },
    "./typescript": {
      "import": "./dist/typescript.js"
    },
    "./jsx": {
      "import": "./dist/jsx.js"
    },
    "./import": {
      "import": "./dist/import.js"
    },
    "./unused-imports": {
      "import": "./dist/unused-imports.js"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint"
  },
  "peerDependencies": {
    "eslint": ">= 9"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^2.6.0-beta.0",
    "@typescript-eslint/parser": "^8.0.0-alpha.40",
    "eslint-plugin-unused-imports": "^4.0.0",
    "typescript-eslint": "^8.0.0-alpha.39"
  },
  "devDependencies": {
    "@types/eslint__js": "^8.42.3",
    "eslint": "^9.6.0",
    "typescript": "^5.5.3"
  }
}
